    "version": "0.2.0",
    "configurations": [
        {
            "name": "smoke_log",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port",
                "5000"
            ]
        },
        {
            "name": "smoke_log (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "smoke_log (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Clone JacobLogs",
            "type": "dart",
            "request": "launch",
            "program": "lib/scripts/clone_jacob_logs.dart",
            "args": ["<testUserId>"]
        }
    ]
}